{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@views/*": [
        "views/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@stores/*": [
        "stores/*"
      ],
      "@router/*": [
        "router/*"
      ],
    },
    "types": [
      "node"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}